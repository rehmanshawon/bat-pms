{
  "name": "bat-pms",
  "version": "0.0.1",
  "description": "BAT Performance Management System",
  "author": "Apsis Solutions Ltd.",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compodoc": "npx compodoc -p tsconfig.json -s"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.1.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^6.11.5",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/terminus": "^8.0.1",
    "@nestjs/websockets": "^6.11.5",
    "@nestjsplus/config": "^1.0.14",
    "@nestjsplus/massive": "^1.2.0",
    "@squareboat/nest-console": "0.0.7",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "crypto": "^1.0.1",
    "currency-formatter": "^1.5.8",
    "exceljs": "^4.3.0",
    "express-basic-auth": "^1.2.0",
    "ioredis": "^4.14.1",
    "jwt-decode": "^3.1.2",
    "knex": "^0.95.11",
    "luxon": "^2.0.2",
    "mysql": "^2.18.1",
    "nestjs-i18n": "^8.1.6",
    "oracledb": "^5.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "php-date": "^3.0.1",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.15",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager-redis-store": "^2.0.0",
    "@types/currency-formatter": "^1.5.1",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.14.6",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.173",
    "@types/luxon": "^2.0.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/RehmanShawon/bat-pms.git"
  },
  "bugs": {
    "url": "https://gitlab.com/RehmanShawon/bat-pms/issues"
  },
  "homepage": "https://gitlab.com/RehmanShawon/bat-pms#readme"
}
